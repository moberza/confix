@MAINTAINER_TRUE@generate:	clean htmldoc/index.html htmldoc/ac-archive.css htmldoc/notfound.html
@MAINTAINER_TRUE@	@echo Generating Archive ...
@MAINTAINER_TRUE@	@$(MAKE) -s MAKEFLAGS= -C tools genarchive
@MAINTAINER_TRUE@	@tools/genarchive $(M4SOURCE)
