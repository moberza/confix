@MAINTAINER_TRUE@	@echo Pretty-printing HTML files ...
@MAINTAINER_TRUE@	@for n in `find htmldoc -name '*.html'`; do $(TIDY) $$n; done

@MAINTAINER_TRUE@	@echo Reconfigure Build System
@MAINTAINER_TRUE@	@./config.status --recheck

