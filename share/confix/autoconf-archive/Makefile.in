@MAINTAINER_TRUE@	@redate configure.ac README

@MAINTAINER_TRUE@publish:	dist
@MAINTAINER_TRUE@	cp $(distdir).tar.bz2 htmldoc/
@MAINTAINER_TRUE@	rsync -va htmldoc/ \
@MAINTAINER_TRUE@	  /usr/local/apache/htdocs/cryp.to/autoconf-archive/

